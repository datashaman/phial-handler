{
    "name": "datashaman/phial-handler",
    "description": "Handler for phial.",
    "license": "MIT",
    "require": {
        "adbario/php-dot-notation": "^2.2",
        "circli/event-dispatcher": "^2.2",
        "container-interop/service-provider": "^0.4.0",
        "kriswallsmith/buzz": "^1.1",
        "nyholm/psr7": "^1.3",
        "php-di/invoker": "^2.1",
        "pkerrigan/xray": "^1.2",
        "psr/event-dispatcher": "^1.0",
        "psr/http-message": "^1.0",
        "psr/log": "^1.1"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Datashaman\\Phial\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
