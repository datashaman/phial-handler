{
    "name": "datashaman/phial-handler",
    "description": "Handler for phial.",
    "license": "MIT",
    "require": {
        "kriswallsmith/buzz": "^1.1",
        "nyholm/psr7": "^1.3",
        "php-di/invoker": "^2.1",
        "psr/event-dispatcher": "^1.0",
        "psr/http-message": "^1.0",
        "psr/http-server-handler": "^1.0",
        "psr/http-server-middleware": "^1.0",
        "psr/log": "^1.1"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Datashaman\\Phial\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
